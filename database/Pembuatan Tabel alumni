CREATE TABLE alumni (
    id INT AUTO_INCREMENT PRIMARY KEY,         -- Primary key (unik, auto naik)
    nama VARCHAR(100) NOT NULL,
    nik VARCHAR(20) NOT NULL UNIQUE,           -- Tidak boleh sama antar data
    nisn VARCHAR(20) NOT NULL UNIQUE,          -- Tidak boleh sama antar data
    tempat_lahir VARCHAR(50) NOT NULL,
    tanggal_lahir DATE NOT NULL,
    alamat TEXT NOT NULL,
    tahun_lulus INT(4) NOT NULL,
    jurusan VARCHAR(50) NOT NULL
);
